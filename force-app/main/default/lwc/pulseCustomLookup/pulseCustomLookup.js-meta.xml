<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>60.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightning__FlowScreen</target>
        <target>lightning__HomePage</target>
    </targets>
    <description>Customlookup component.</description>
	<masterLabel>pulse-custom-lookup</masterLabel>
	<targetConfigs>
        <targetConfig targets="lightning__FlowScreen">
            <property label="Record Type" name="recordTypeId" type="String" />
            <property label="Object API Name" name="objectApiName" type="String" />
            <property label="Field API Name" name="fieldName" type="String" />
            <property label="Label" name="label" type="String" />
            <property label="Default Value" name="defaultValue" type="String" />
            <property label="record Id" name="recordId" type="String" role="outputOnly" />    
            <property label="Label for add button" name="labelForAddButton" type="String" />      
        </targetConfig>
    </targetConfigs>
    
</LightningComponentBundle>